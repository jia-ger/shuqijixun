https://atcoder.jp/contests/abc304/tasks/abc304_d?lang=en

#include <bits/stdc++.h>
using namespace std;
#define itn int
#define icn cin
#define int long long
#define endl '\n'
typedef pair<int,int> PII;
const int N=2e5+10;
const int INF=1e18;
const int mod=1e9+7;
#define ll __int128

struct abd{
	int x,y;
}p[N];
int a[N],b[N];
map<PII,int>mp;
void solve()
{ 
   int w,h;cin>>w>>h;
   itn n;cin>>n;
   for(int i=1;i<=n;i++)
   cin>>p[i].x>>p[i].y;
   int A;cin>>A;
   for(int i=1;i<=A;i++)cin>>a[i];
   int B;cin>>B;
   for(int i=1;i<=B;i++)cin>>b[i];
   
   int num=0;
   int ans1=INT_MAX;
   int ans2=0;
   for(int i=1;i<=n;i++)
   {
   	int h=lower_bound(a+1,a+A+1,p[i].x)-a;
   	int w=lower_bound(b+1,b+B+1,p[i].y)-b;
   	mp[{h,w}]++;
   	if(mp[{h,w}]==1)num++;
   }
   for(int i=1;i<=n;i++)
   {
   	int h=lower_bound(a+1,a+A+1,p[i].x)-a;
   	int w=lower_bound(b+1,b+B+1,p[i].y)-b;
   	ans1=min(ans1,mp[{h,w}]);
   	ans2=max(ans2,mp[{h,w}]);
   }
   if(num<(A+1)*(B+1))cout<<0<<" "<<ans2;
   else cout<<ans1<<" "<<ans2;
} 


signed main(){
   cin.tie(0)->sync_with_stdio(0);	 
   int T=1; 
//   cin>>T;
   while(T--)solve();
   
	return 0;
}
  
