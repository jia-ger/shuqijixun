https://atcoder.jp/contests/abc296/tasks/abc296_e

//当ki很大只有环可能成立
#include <bits/stdc++.h>
using namespace std;
#define itn int
#define icn cin
#define int long long
#define endl '\n'
typedef pair<int,int> PII;
const int N=2e5+10;
const int INF=1e18;
const int mod=998244353;
#define ll __int128

itn n;
int a[N];
int cnt[N];
int final(int x)
{   
    int ans=0;
    queue<int>q;
	for(int i=1;i<=x;i++)
	if(cnt[i]==0)q.push(i);
	while(!q.empty())
	{
		auto t=q.front();
		q.pop();
		ans++;
		cnt[a[t]]--;
		if(cnt[a[t]]==0)
		q.push(a[t]);
	}
	return ans;
}
void solve()
{   
    cin>>n;
    for(itn i=1;i<=n;i++)cin>>a[i],cnt[a[i]]++;
    cout<<n-final(n);//当所选的k构成环时 高桥获胜 总数-构不成环的情况即答案 
} 


signed main(){
   cin.tie(0)->sync_with_stdio(0);	 
   int T=1; 
//   cin>>T;
   while(T--)solve();
   
    return 0;
}
