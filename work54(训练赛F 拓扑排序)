https://atcoder.jp/contests/abc291/tasks/abc291_e?lang=en

#include <bits/stdc++.h>
using namespace std;
#define itn int
#define icn cin
#define int long long
#define endl '\n'
typedef pair<int,int> PII;
const int N=3e5+10;
const int INF=1e18;
const int mod=998244353;
#define ll __int128

int n,m,d[N],cnt,a[N],w;
vector<int> v[N];
queue<int> q;
void solve()
{   
 cin>>n>>m;
 for(int i=1;i<=m;i++)
  {
  	int x,y;icn>>x>>y;
  	v[x].push_back(y);
  	d[y]++;
  }
  for(int i=1;i<=n;i++)if(!d[i])q.push(i);
  if(q.size()!=1)
  {
  	cout<<"No";return;
  }
  while(!q.empty())
  {
	auto x=q.front();
	q.pop();
	a[x]=++cnt,w=0;
		for(int t:v[x]){
			d[t]--;
			if(!d[t])q.push(t),w++;
			if(w>=2)
			{
  	         cout<<"No";return;
            }
		}
  }
	if(cnt!=n)
	{
  	cout<<"No";return;
    }
	cout<<"Yes"<<endl;
	for(int i=1;i<=n;i++)cout<<a[i]<<" ";
} 


signed main(){
   cin.tie(0)->sync_with_stdio(0);	 
   int T=1; 
//   cin>>T;
   while(T--)solve();
   
    return 0;
}
